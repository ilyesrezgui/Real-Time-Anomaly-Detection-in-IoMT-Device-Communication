      "targets": [
        {
          "query": "// Count predicted anomalies that are actually attacks\ntrue_positives = from(bucket: \"iomt_data\")\n  |> range(start: -24h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"iomt_traffic\")\n  |> filter(fn: (r) => r[\"predicted_anomaly\"] == \"true\")\n  |> filter(fn: (r) => r[\"label\"] != \"BENIGN\")\n  |> filter(fn: (r) => r[\"_field\"] == \"rate\")\n  |> group()\n  |> count()\n  |> map(fn: (r) => ({r with _field: \"tp\"}))\n\n// Count all actual attacks\ntotal_attacks = from(bucket: \"iomt_data\")\n  |> range(start: -24h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"iomt_traffic\")\n  |> filter(fn: (r) => r[\"label\"] != \"BENIGN\")\n  |> filter(fn: (r) => r[\"_field\"] == \"rate\")\n  |> group()\n  |> count()\n  |> map(fn: (r) => ({r with _field: \"total\"}))\n\nunion(tables: [true_positives, total_attacks])\n  |> pivot(rowKey:[], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> map(fn: (r) => ({_value: float(v: r.tp) / float(v: r.total) * 100.0}))",
          "refId": "A"
        }
      ],
      "title": "True Positive Rate (%)",
      "type": "stat"
    },
